body	{
		background: url('https://pinchypants.files.wordpress.com/2016/02/minecraft.jpeg') no-repeat center center fixed;
		-webkit-background-size: cover;
		-moz-background-size: cover;
		-o-background-size: cover;
		background-size: cover;
}

.bitDown	{

	margin-top: 10px;

}

.wrapper	{

	padding: 5px;
	background-color: white;
	border: 5px solid #43AC6A;

}

/*alerts*/

.alert-success {

	background: white;
	border: 5px solid #43AC6A;
	border-radius: 0;
	font-style: italic;
	font-family: mcsmall;
	color: black;

}

.alert-danger {

	background: #DC143C;
	border: 5px solid white;
	border-radius: 0;
	text-align: center;
	font-style: italic;
	font-family: mcsmall;

}

/*Panel*/
.panel-default > .panel-heading {

		color: white;
		background: #43AC6A;
		border-radius: 0;
		min-height: 40px;

}
.panel-default .panel-heading, .panel-default .panel-title, .panel-default .panel-footer {
    color: white;
}
.panel-title{
	font-size: 20px !important;
	font-weight: normal !important;
	-webkit-font-smoothing: antialiased !important;
}
.panel-default	{

		border-radius: 0;
		border: 1px solid #43AC6A;
		-webkit-box-shadow: 0px 6px 5px 0px rgba(50, 50, 50, 0.30);
		-moz-box-shadow:    0px 6px 5px 0px rgba(50, 50, 50, 0.30);
		box-shadow:         0px 6px 5px 0px rgba(50, 50, 50, 0.30);

}

.panel-heading	{

		font-family: minecraft;
		border-radius: 0;

}

.panel-body	{

	font-family: mcsmall;

}

.panel-danger > .panel-heading {

		color: white;
		background: #DC143C;
		border-radius: 0;

}
.panel-danger	{

		border-radius: 0;

}

/*Buttons*/
.btn-primary {

		color: #ffffff;
		background-color: #43AC6A;
		border: none;
		transition: all .5s linear;

}
.btn-primary:focus,
.btn-primary.focus {

		color: #ffffff;
		background-color: #43AC6A;
		border: none;
		opacity: .7;
		transition: all .5s linear;

}
.btn-primary:hover {

		color: #ffffff;
		background-color: #43AC6A;
		border: none;
		opacity: .7;
		transition: all .5s linear;

}

/*Navbar*/

.navbar-default .navbar-toggle {
		border-color: white;
}
.navbar-default .navbar-toggle:hover,
.navbar-default .navbar-toggle:focus {
		background-color: #43AC6A;
}
.navbar-default .navbar-toggle .icon-bar {
		background-color: white;
}
.navbar-default .navbar-brand {
		color: white;
		font-family: minecraft;
}
.navbar-default .navbar-brand:hover,
.navbar-default .navbar-brand:focus {
		color: white;
		background-color: transparent;
}
.navbar-default, .dropdown-menu {
		border: none;
		border-radius: 0;
		background: #43AC6A;
		-webkit-box-shadow: 0px 6px 5px 0px rgba(50, 50, 50, 0.30);
		-moz-box-shadow:    0px 6px 5px 0px rgba(50, 50, 50, 0.30);
		box-shadow:         0px 6px 5px 0px rgba(50, 50, 50, 0.30);
}
.navbar-default .navbar-nav > li > a,
.dropdown-menu > li > a {
		color: white;
		font-size: 14px;
		font-family: minecraft;
		background: #43AC6A;
}
.navbar-default .navbar-nav > li > a:hover,
.navbar-default .navbar-nav > li > a:focus,
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus {
		color: white;
		background: #393A35;
		text-shadow: -1px 1px 8px #ffc, 1px -1px 8px #fff;
}

/*Footer*/
.footer {

    height: auto;
    line-height: 40px;
    margin-bottom: 50px;
    background-color: #43AC6A;
	padding-top: 5px;
	margin-top: 20px;
	-webkit-box-shadow: 0px 6px 5px 0px rgba(50, 50, 50, 0.30);
	-moz-box-shadow:    0px 6px 5px 0px rgba(50, 50, 50, 0.30);
	box-shadow:         0px 6px 5px 0px rgba(50, 50, 50, 0.30);

}

.footerText	{

	font-family: mcsmall;
	color: white;

}

.footerText a {

	color: white;

}


.formbg	{
		background: url('/assets/img/dirtbg.png');
		-webkit-background-size: auto;
		-moz-background-size: auto;
		-o-background-size: auto;
		background-size: auto;
}

.formbg	form h2 {
	color: #A0A0A0;
}

.formbg	form input[type=text], .formbg form input[type=password] {
	background-color: black;
	border: 2px solid #A0A0A0 !important;
	border-radius: 0px;
	margin-bottom: 10px;
	color: white;
}

.formbg	form input[type=submit], .formbg form button{
	background-color: #6F6F6F !important;
	border: 2px solid black !important;
	border-radius: 0px;
	margin-bottom: 10px;
	color: white;
	font-size: 20px !important;
	white-space: normal !important;
}

.formbg	form input[type=submit]:hover, .formbg form button:hover{
	background-color: #7E88BF !important;
}

.floating-accountbar{
	position: absolute;
	float: left;
	width: 100%;
}

.head-accountbar{
	background-color: #43AC6A;
}

.head-accountbar li a{
	color: white;
  font-size: large;
}

.head-accountbar li a:hover{
	color: #FFF;
	background-color: #393A35;
}

.nav .open>a, .nav .open>a:hover, .nav .open>a:focus {
    background-color: #393A35;
    border-color: #93c54b;
}

.btn1, .btn2{
	height: 120px;
}

.row-striped .row:nth-of-type(odd) {
    background: #ECF8E0;
}
.row-striped .row:nth-of-type(even) {
    background: #FFFFFF;
}
.new-line{
	display: flex;
	font-family: mcsmall;
}

.textarea200 {
  min-height: 200px;
	height: 200px;
	font-size: large;
}
.quote {
    padding: 15px;
    border-left: 3px solid #aaa;
    color: #777;
}
blockquote{
		font-size: 13px !important;
}

@media screen and (min-width: 768px) {
	.center {
	    margin: auto;
	    width: 50%;
	    float: none;
	}
}

@media (max-width: 767px){
	.navbar-default .navbar-nav .open .dropdown-menu>li>a {
	    color: #ffffff;
	}
}


